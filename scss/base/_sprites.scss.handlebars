{{#each sprites}}{{#if @index}}, {{/if}}.{{name}}{{/each}} {
    display: inline-block;
    background-image: url({{spritesheet.image}});
    background-repeat: no-repeat;
}

{{#sprites}}
.{{name}} {
    background-position: {{px.offset_x}} {{px.offset_y}};
    width: {{px.width}};
    height: {{px.height}};
}
{{/sprites}}
